{
  "name": "org-asm",
  "version": "0.1.0",
  "description": "Rust-first MVC for React applications. All logic that can live in Rust, lives in Rust â€” state, validation, computation, business rules. TypeScript is the thin rendering and I/O layer.",
  "type": "module",
  "bin": {
    "org-asm": "./bin/org-asm.mjs"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./index.ts",
      "import": "./dist/index.js",
      "default": "./index.ts"
    },
    "./core": {
      "types": "./core/index.ts",
      "import": "./dist/core/index.js",
      "default": "./core/index.ts"
    },
    "./model": {
      "types": "./model/index.ts",
      "import": "./dist/model/index.js",
      "default": "./model/index.ts"
    },
    "./view": {
      "types": "./view/index.ts",
      "import": "./dist/view/index.js",
      "default": "./view/index.ts"
    },
    "./controller": {
      "types": "./controller/index.ts",
      "import": "./dist/controller/index.js",
      "default": "./controller/index.ts"
    },
    "./react": {
      "types": "./react/index.ts",
      "import": "./dist/react/index.js",
      "default": "./react/index.ts"
    },
    "./vite": {
      "types": "./vite/index.ts",
      "import": "./dist/vite/index.js",
      "default": "./vite/index.ts"
    }
  },
  "files": [
    "bin",
    "codegen",
    "dist",
    "schema",
    "model/engine-template.rs",
    "model/form-engine-template.rs",
    "model/table-engine-template.rs",
    "model/auth-engine-template.rs",
    "model/router-engine-template.rs",
    "model/history-engine-template.rs",
    "model/intl-engine-template.rs",
    "model/search-engine-template.rs",
    "model/statemachine-engine-template.rs",
    "model/api-engine-template.rs",
    "model/virtualscroll-engine-template.rs",
    "model/validation-engine-template.rs",
    "model/selection-engine-template.rs",
    "model/commandpalette-engine-template.rs",
    "model/Cargo.template.toml",
    "shared/lib-template.rs",
    "shared/validation-template.rs",
    "shared/Cargo.template.toml",
    "server/engine-trait.rs",
    "server/broadcast.rs",
    "server/main-template.rs",
    "server/command-handler-template.rs",
    "server/Cargo.template.toml",
    "vite/index.ts",
    "vite/rustServerPlugin.ts",
    "vite/types.ts",
    "guides"
  ],
  "scripts": {
    "build": "tsc && tsc -p tsconfig.vite.json",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "wasm",
    "webassembly",
    "rust",
    "react",
    "realtime",
    "animation",
    "60fps",
    "frame-buffer",
    "zero-copy",
    "mvc",
    "server",
    "orderbook",
    "websocket",
    "validation",
    "forms",
    "state-management",
    "data-table",
    "auth",
    "router",
    "undo-redo",
    "i18n",
    "search",
    "state-machine",
    "api",
    "virtual-scroll",
    "validation",
    "selection",
    "command-palette"
  ],
  "license": "MIT",
  "dependencies": {
    "flatbuffers": "^24.0.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "rxjs": ">=7.0.0",
    "vite": ">=5.0.0",
    "vite-plugin-top-level-await": ">=1.0.0",
    "vite-plugin-wasm": ">=3.0.0",
    "zustand": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    },
    "vite-plugin-wasm": {
      "optional": true
    },
    "vite-plugin-top-level-await": {
      "optional": true
    }
  },
  "devDependencies": {
    "@testing-library/react": "^16.3.2",
    "@types/node": "^22.0.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitest/coverage-v8": "^4.0.18",
    "happy-dom": "^20.6.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "rxjs": "^7.8.0",
    "typescript": "^5.7.0",
    "vite": "^7.3.1",
    "vite-plugin-top-level-await": "^1.6.0",
    "vite-plugin-wasm": "^3.5.0",
    "vitest": "^4.0.18",
    "zustand": "^5.0.0"
  }
}
